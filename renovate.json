{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"local>CPS-IT/renovate-config"
	],
	"assignees": [
		"eliashaeussler"
	],
	"constraints": {
		"php": "8.2.*"
	},
	"packageRules": [
		{
			"extends": [
				":automergeDisabled"
			],
			"matchPackageNames": [
				"composer/composer"
			],
			"matchUpdateTypes": [
				"major",
				"minor"
			]
		}
	]
}
